@@ -166,7 +166,7 @@ create_test_component (time_t start,
 	e_cal_component_set_dtend (comp, &dtend);
 	*/
 
-	summary.value = g_strdup_printf ("%ld - %ld", start, end);
+	summary.value = g_strdup_printf ("%d - %d", start, end);
 	summary.altrep = NULL;
 
