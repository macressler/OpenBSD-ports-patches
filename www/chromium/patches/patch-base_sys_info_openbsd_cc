+
+// static
+std::string SysInfo::CPUModelName() {
+  int mib[] = { CTL_HW, HW_MODEL };
+  char name[256];
+  size_t len = arraysize(name);
+  if (sysctl(mib, arraysize(mib), &name, &len, NULL, 0) == 0)
+    return name;
+  return std::string();
 }
 
 }  // namespace base
