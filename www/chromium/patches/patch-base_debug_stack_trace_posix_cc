@@ -473,7 +473,11 @@ StackTrace::StackTrace() {
 
   // Though the backtrace API man page does not list any possible negative
   // return values, we take no chance.
+#if defined(OS_BSD)
+  count_ = 0;
+#else
   count_ = std::max(backtrace(trace_, arraysize(trace_)), 0);
+#endif
 }
 
